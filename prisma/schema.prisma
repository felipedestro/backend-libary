generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model books {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  company    String
  isbn       Int
  pages      Int
  title      String
  created_at DateTime? @default(now()) @db.Date
  updated_at DateTime? @default(now()) @db.Date
}
