generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model livros {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  editora    String
  isbn        Int
  num_paginas  Int
  titulo      String
  created_at DateTime? @default(now()) @db.Date
  updated_at DateTime? @default(now()) @db.Date
}
